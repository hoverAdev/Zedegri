i = 0;
function Assign_Item(All_Items, Catalogue){	
	for (var incrementor = 0; incrementor < array_length(Catalogue); incrementor++){
		if All_Items[i] == Catalogue[incrementor].name{
			i++;
			return Catalogue[incrementor];
		}
	}
}


